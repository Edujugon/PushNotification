{
    "name": "edujugon/push-notification",
    "description": "Laravel 5 Package to send push notifications to Android and IOS devices. (GCM,FCM,APN)",
    "keywords": ["Laravel","push","notification","gcm","apn","fcm","high","priority"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Marcos",
            "email": "edujugon@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.3",
        "illuminate/notifications": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Edujugon\\PushNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edujugon\\PushNotification\\Providers\\PushNotificationServiceProvider"
            ],
            "aliases": {
                "PushNotification": "Edujugon\\PushNotification\\Facades\\PushNotification"
            }
        }
    },
    "minimum-stability": "dev"
}
